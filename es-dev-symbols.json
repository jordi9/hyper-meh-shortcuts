{
    "description": "ES Dev Symbols - Meh & Hyper Layers",
    "manipulators": [
        {
            "type": "basic",
            "description": "Caps Lock → Meh (held) / Escape (tapped)",
            "from": {
                "key_code": "caps_lock",
                "modifiers": { "optional": ["any"] }
            },
            "to": [
                {
                    "key_code": "left_shift",
                    "modifiers": ["left_control", "left_option"]
                }
            ],
            "to_if_alone": [{ "key_code": "f13" }]
        },
        {
            "type": "basic",
            "description": "Meh + H → `",
            "from": {
                "key_code": "h",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "open_bracket", "modifiers": [] }, { "key_code": "spacebar", "modifiers": [] }]
        },
        {
            "type": "basic",
            "description": "Meh + L → |",
            "from": {
                "key_code": "l",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "1", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + U → (",
            "from": {
                "key_code": "u",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "8", "modifiers": ["shift"] }]
        },
        {
            "type": "basic",
            "description": "Meh + I → )",
            "from": {
                "key_code": "i",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "9", "modifiers": ["shift"] }]
        },
        {
            "type": "basic",
            "description": "Meh + J → [",
            "from": {
                "key_code": "j",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "open_bracket", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + K → ]",
            "from": {
                "key_code": "k",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "close_bracket", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + M → {",
            "from": {
                "key_code": "m",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "quote", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + , → }",
            "from": {
                "key_code": "comma",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "backslash", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + N → @",
            "from": {
                "key_code": "n",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "2", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + . → #",
            "from": {
                "key_code": "period",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [{ "key_code": "3", "modifiers": ["option"] }]
        },
        {
            "type": "basic",
            "description": "Meh + ñ → ~",
            "from": {
                "key_code": "semicolon",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [
                { "key_code": "semicolon", "modifiers": ["option"] },
                { "key_code": "spacebar", "modifiers": [] }
            ]
        },
        {
            "type": "basic",
            "description": "Meh + Y → ->",
            "from": {
                "key_code": "y",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [
                { "key_code": "slash", "modifiers": [] },
                { "key_code": "non_us_backslash", "modifiers": ["shift"] }
            ]
        },
        {
            "type": "basic",
            "description": "Meh + O → =>",
            "from": {
                "key_code": "o",
                "modifiers": { "mandatory": ["control", "shift", "option"] }
            },
            "to": [
                { "key_code": "0", "modifiers": ["shift"] },
                { "key_code": "non_us_backslash", "modifiers": ["shift"] }
            ]
        }
    ]
}
